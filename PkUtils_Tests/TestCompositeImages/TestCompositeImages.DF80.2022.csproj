<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net8.0-windows</TargetFramework>
    <OutputType>WinExe</OutputType>
    <RootNamespace>TestCompositeImages</RootNamespace>
    <AssemblyName>TestCompositeImages</AssemblyName>
