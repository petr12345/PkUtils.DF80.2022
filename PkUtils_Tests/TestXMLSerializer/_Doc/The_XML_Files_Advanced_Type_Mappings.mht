static void Serialize()
{
    PaymentMethod p =3D new PaymentMethod();
    p.Item =3D 7.5;
    // specify which member we're actually using
    p.ItemElementName =3D ItemChoiceType.hourly;

    XmlSerializer s =3D new XmlSerializer(typeof(PaymentMethod));
    FileStream fs =3D new FileStream("p.xml", FileMode.Create);
    s.Serialize(fs, p);
